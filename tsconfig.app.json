{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.json",
    "src/*.vue",
    "*.json",
  ],
  "exclude": [
    "src/**/__tests__/*",
    "src/tools/freshCountryFlags.ts",
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "#/*": [
        "./*"
      ]
    }
  }
}